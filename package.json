{
  "name": "react-detect-offline",
  "version": "2.4.0",
  "description": "Offline and Online components for React",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisbolin/react-detect-offline.git"
  },
  "files": ["/dist/"],
  "author": "Chris Bolin",
  "license": "MIT",
  "types": "types.d.ts",
  "devDependencies": {
    "@testing-library/react": "^8.0.7",
    "@babel/cli": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^4.19.1",
    "eslint-config-formidable": "^3.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^24.8.0",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react": "^16.9.0-alpha.0",
    "react-dom": "^16.9.0-alpha.0"
  },
  "bugs": {
    "url": "https://github.com/chrisbolin/react-detect-offline/issues"
  },
  "homepage": "https://github.com/chrisbolin/react-detect-offline#readme",
  "scripts": {
    "build": "babel src/**/*.js --out-dir dist",
    "test": "jest spec",
    "test-watch": "jest spec --watch",
    "lint": "eslint src/ demo/src",
    "prettier": "prettier src/** demo/src/** --write",
    "test.cover": "jest spec --coverage"
  },
  "keywords": ["react", "offline", "online", "detect", "disconnect"],
  "jest": {
    "setupFilesAfterEnv": ["@testing-library/react/cleanup-after-each"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/demo/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
